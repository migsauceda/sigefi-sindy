--modificacion al aumento del valor del tipo de dato del estado de la denuncia en la tabla denuncia--
ALTER TABLE mini_sedi.tbl_denuncia ALTER COLUMN cestadodenuncia SET DATA TYPE character varying(2);


--CREACION DE LA TABLA ESTADO--
CREATE TABLE mini_sedi.tbl_estado(cestadodenuncia character varying(2),cdescripcion character varying(50),
	CONSTRAINT pk_estados PRIMARY KEY (cestadodenuncia)
    );
