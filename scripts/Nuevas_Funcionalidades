--indica cestadodenuncia de la tabla denuncia es una llave foranea--
ALTER TABLE mini_sedi.tbl_denuncia
   ADD CONSTRAINT fk_denuncia
   FOREIGN KEY (cestadodenuncia) 
   REFERENCES mini_sedi.tbl_estado(cestadodenuncia);

--creacion de la tabla intermedia estado_actividad--
CREATE TABLE mini_sedi.tbl_estado_actividad
(
    cestadodenuncia character varying(2),
    nactividadid integer NOT NULL,
    cnuevoestado character varying(2),
    CONSTRAINT pk_estado_actividad PRIMARY KEY (cestadodenuncia, nactividadid),
    CONSTRAINT fk_actividad FOREIGN KEY (nactividadid)
        REFERENCES mini_sedi.tbl_actividad (nactividadid) MATCH SIMPLE
        ON UPDATE NO ACTION
        ON DELETE NO ACTION,
    CONSTRAINT fk_estados FOREIGN KEY (cestadodenuncia)
        REFERENCES mini_sedi.tbl_estado (cestadodenuncia) MATCH SIMPLE
        ON UPDATE NO ACTION
        ON DELETE NO ACTION
);

-- creacion de la tabla intermedia materia_actividad--
CREATE TABLE mini_sedi.tbl_materia_actividad
(    nactividadid integer NOT NULL,
     netapaid integer NOT NULL,
     nsubetapaid integer NOT NULL,
     nmateria integer NOT NULL,

    CONSTRAINT pk_materia_actividad PRIMARY KEY (nactividadid, netapaid, nsubetapaid,nmateria),
    CONSTRAINT fk_actividad FOREIGN KEY (nactividadid)
        REFERENCES mini_sedi.tbl_actividad (nactividadid) MATCH SIMPLE
        ON UPDATE NO ACTION
        ON DELETE NO ACTION,
    CONSTRAINT fk_etapa FOREIGN KEY (netapaid)
        REFERENCES mini_sedi.tbl_etapa (netapaid) MATCH SIMPLE
        ON UPDATE NO ACTION
        ON DELETE NO ACTION,
   CONSTRAINT fk_subetapa FOREIGN KEY (nsubetapaid)
        REFERENCES mini_sedi.tbl_subetapa (nsubetapaid) MATCH SIMPLE
        ON UPDATE NO ACTION
        ON DELETE NO ACTION,
CONSTRAINT fk_materia FOREIGN KEY (nmateria)
        REFERENCES mini_sedi.tbl_materia (nmateria) MATCH SIMPLE
        ON UPDATE NO ACTION
        ON DELETE NO ACTION


);
